{
  "name": "Google Search Highlighter",
  "version": "0.1",
  "permissions": [
    "<all_urls>"
  ],
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" 
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["sample.js"],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}